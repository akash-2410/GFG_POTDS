 public void Rearrange(int a[], int n)
    {
        // Your code goes here
   ArrayList<Integer> temp=new ArrayList<>();
     for(int i=0;i<n;i++){
         if(a[i]<0)temp.add(a[i]);
      }
     for(int i=0;i<n;i++){
         if(a[i]>=0)temp.add(a[i]);
     }
     int i=0; 
     for(int x:temp){
     a[i++]=x;
    }
    }
